{
  "manifest_version": 2,
  "name": "Extension Test",
  "description": "Extension To Do Something",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "camera-icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content-scripts": "*"[{
    "messages": ["*"],
    "js": ["popup.js"]
  }],
  "permissions": ["activeTab"]
}
